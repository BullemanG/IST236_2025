{
  "expo": {
    "name": "campground-reservations",
    "slug": "campground-reservations",
    "version": "1.0.0",
    "orientation": "automatic",
    "icon": "./assets/images/icon.jpg",
    "splash": {
      "image": "./assets/images/camping-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/camping-splash.jpg",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
